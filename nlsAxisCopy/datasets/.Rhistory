getSurface <- function(ind, certainStack) {
allPts <- rasterToPoints(certainStack[[ind]], function(x){x==255})[,-3]
n <- nrow(allPts)
surfacePts <- allPts[sample(n, 0.05*n, replace=FALSE),]
return(surfacePts)}
